<beans xmlns:jms="jms://servicemix.apache.org/jms/1.0"
       xmlns:ping="urn:/Ping.wsdl"
       xmlns:pong="urn:/Pong.wsdl"
    >
    <jms:endpoint 
        service="ping:PingService"
        endpoint="jms" 
	targetService="ping:PingService"
	targetEndpoint="PingPort"
        defaultOperation="Ping"
        role="consumer"
        locationURI="jms://localhost:8192/Pingjms/"
        defaultMep="jms://www.w3.org/2004/08/wsdl/in-out" 
	wsdlResource="classpath:Ping.wsdl"
	soapVersion="1.1"
        soap="true" />
</beans>
